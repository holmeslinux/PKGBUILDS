# Maintainer: vinceliuice

pkgname=gtk-theme-stylishdark
pkgver=0.6
pkgrel=1
pkgdesc="StylishDark Theme StylishDark-et, StylishDark-wpp and StylishDark-wps"
arch=('any')
url="http://vinceliuice.deviantart.com/art/StylishDark-499195225"
license=('GPL')
depends=('gtk2' 'gtk-engine-murrine>=0.98.2')
source=('http://www.deviantart.com/download/499195225/stylishdark_by_vinceliuice-d897hi1.7z?token=3a7207099e0ed245aa61012b1930c235f8fac45f&ts=1457640051')
md5sums=('304702f57970fda3e279da285e939750')

package() {
# create theme dirs
  install -d -m 755 "$pkgdir"/usr/share/themes/StylishDark

 # install theme
  cd $srcdir/$_pkgname/gtk-theme-stylishdark
  cp -r . "$pkgdir"/usr/share/themes/StylishDark	
}
